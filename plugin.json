{
  "version": "1.0.6",
  "platform": ["win32"],
  "pluginName": "vmware启动助手",
  "description": "打开|开启|关闭本地虚拟机",
  "preload": "preload.js",
  "logo": "logo.png",
  "features": [
    {
      "platform": ["win32"],
      "code": "vmware_init",
      "explain": "初始化vmware插件",
      "cmds": ["vmware_init", "vminit", "vmware插件初始化"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_open",
      "explain": "选择虚拟机并打开",
      "cmds": ["vmware","vmware_open", "vmopen"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_run",
      "explain": "选择虚拟机并启动",
      "cmds": ["vmware_run", "vmrun"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_stop",
      "explain": "选择虚拟机并关闭",
      "cmds": ["vmware_stop", "vmstop"]
    }
  ]
}