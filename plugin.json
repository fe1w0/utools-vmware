{
  "version": "1.0.7",
  "platform": ["win32"],
  "logo": "logo.png",
  "author": "fe1w0",
  "pluginName": "vmware启动助手",
  "description": "打开|开启|关闭本地虚拟机|",
  "preload": "preload.js",
  "development": {
  },
  "features": [
    {
      "platform": ["win32"],
      "code": "vmware_init",
      "explain": "初始化vmware插件",
      "cmds": ["vmware_init", "vminit", "vmware插件设置"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_open",
      "explain": "选择虚拟机并打开",
      "cmds": ["vmware","vmware_open", "vmopen"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_run",
      "explain": "选择虚拟机并启动",
      "cmds": ["vmware_run", "vmrun"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_stop",
      "explain": "选择正在运行虚拟机并关闭",
      "cmds": ["vmware_stop", "vmstop"]
    },
    {
      "platform": ["win32"],
      "code": "vmware_suspend",
      "explain": "选择正在运行虚拟机并挂起",
      "cmds": ["vmware_suspend", "vmsuspend"]
    }
  ]
}